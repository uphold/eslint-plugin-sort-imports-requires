{
  "name": "eslint-plugin-sort-imports-requires",
  "version": "2.0.0",
  "description": "An ESLint plugin to sort both import and require declarations in a unified manner.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "sort",
    "import",
    "require",
    "cjs",
    "module"
  ],
  "author": "Uphold",
  "type": "commonjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "repository": "github:uphold/eslint-plugin-sort-imports-requires",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint",
    "release": "release-it",
    "test": "node --test tests/sort-*.js",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@uphold/github-changelog-generator": "^4.0.2",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "release-it": "^19.2.4",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">= 20"
  },
  "peerDependencies": {
    "eslint": ">=9"
  },
  "license": "MIT",
  "dependencies": {
    "@eslint-community/eslint-utils": "^4.9.1"
  }
}
